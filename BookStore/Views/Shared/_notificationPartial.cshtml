
<script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>

<script>
    var msg = @(TempData["Message"]!=null? Html.Raw(TempData["Message"]):Html.Raw("undefined"));

    console.log(msg)

    if (msg) {
        if (msg.provider == "sweetAlert") {
            swal.fire({
                title: msg.title,
                icon: msg.icon
            });
        } else {
            toastr[msg.type](msg.title);
        }

    }



</script>